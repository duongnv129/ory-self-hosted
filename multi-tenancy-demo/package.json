{
  "name": "multi-tenancy-demo",
  "version": "2.0.0",
  "description": "Simple multi-tenancy demo with TypeScript support: users, products, categories, roles",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "dev:watch": "nodemon --exec ts-node src/app.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "test": "node simple-test.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "multi-tenancy",
    "api",
    "express",
    "rest",
    "demo",
    "typescript",
    "ory-kratos",
    "ory-keto"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
